{
    "name": "www.andreadiotallevi.com",
    "private": true,
    "description": "Andrea Diotallevi's personal website",
    "version": "0.1.0",
    "license": "0BSD",
    "scripts": {
        "build": "gatsby build",
        "develop": "env-cmd -f .env.development gatsby develop",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "start": "npm run develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
        "preload-fonts": "gatsby-preload-fonts",
        "type-watch": "tsc --watch --noEmit --project './tsconfig.json'"
    },
    "dependencies": {
        "@fontsource/inter": "^4.5.12",
        "axios": "^1.3.4",
        "dotenv": "^10.0.0",
        "gatsby": "^4.11.1",
        "gatsby-plugin-google-gtag": "^4.9.0",
        "gatsby-plugin-image": "^2.9.0",
        "gatsby-plugin-mailchimp": "^5.2.2",
        "gatsby-plugin-portal": "^1.0.7",
        "gatsby-plugin-react-helmet": "^5.0.0",
        "gatsby-plugin-sass": "^5.0.0",
        "gatsby-plugin-sharp": "^4.9.0",
        "gatsby-remark-external-links": "^0.0.4",
        "gatsby-remark-images": "^6.1.1",
        "gatsby-remark-prismjs": "^6.6.0",
        "gatsby-remark-relative-images": "^2.0.2",
        "gatsby-source-filesystem": "^4.0.0",
        "gatsby-transformer-json": "^4.0.0",
        "gatsby-transformer-remark": "^5.1.1",
        "gatsby-transformer-sharp": "^4.9.0",
        "gbimage-bridge": "^0.2.1",
        "node-sass": "^6.0.1",
        "prismjs": "^1.26.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-intl": "^6.4.2"
    },
    "devDependencies": {
        "@types/node": "^20.2.1",
        "@types/react": "^18.2.6",
        "@types/react-dom": "^18.2.4",
        "@types/react-helmet": "^6.1.6",
        "env-cmd": "^10.1.0",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AndreaDiotallevi/andreadiotallevi.com"
